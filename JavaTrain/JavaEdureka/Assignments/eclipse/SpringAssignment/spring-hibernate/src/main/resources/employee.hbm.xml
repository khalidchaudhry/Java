<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC  
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
      
<hibernate-mapping>  
 <class name="co.edureka.khalid.ems.entity.Employee" table="employee2">  
  <id name="employeeId" column="Id">  
   <generator class="assigned"></generator>  
  </id>                
  <property name="employeeName" column="Name"></property>  
  <property name="Basic" column="Basic"></property>
  <property name="HRA" column="HRA"></property>
  <property name="DA" column="DA"></property>
  <property name="Deductions" column="Deductions"></property>  
 </class>                
</hibernate-mapping> 