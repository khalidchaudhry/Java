<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans.xsd">
	
<!--Value as an attribute-->
<bean id="user1" class="co.edureka.khalid.core.beans.User" init-method="initMethod1" destroy-method="myDestructor">
<property name="firstName" value="Khalid"/>
<property name="lastName" value="Chaudhry"/>
</bean>

<bean id="user2" class="co.edureka.khalid.core.beans.User" init-method="initMethod2" destroy-method="myDestructor">
<constructor-arg value="Babar"></constructor-arg>
<constructor-arg value="Azam"></constructor-arg>

</bean>



</beans>
