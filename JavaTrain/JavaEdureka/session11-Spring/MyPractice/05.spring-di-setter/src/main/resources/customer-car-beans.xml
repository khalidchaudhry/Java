<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans.xsd">
  
  <bean id="bmw" class="co.edureka.khalid.core.beans.Car">
   <property name="model" value="BMW"/>
   <property name="cost" value="200000"/>
  </bean>
  
  <bean id="cust1" class="co.edureka.khalid.core.beans.Customer">
   <property name="firstName" value="Khalid"/>
   <property name="lastName" value="Chaudhry"/>
   <property name="car" ref="bmw"/>   
  </bean>

</beans>
